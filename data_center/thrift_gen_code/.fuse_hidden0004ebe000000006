#ifndef DATABASE_TIMELAPSE_WRAPPER_H
#define DATABASE_TIMELAPSE_WRAPPER_H

#include <kchashdb.h>
#include <string>
#include <vector>

class DatabaseTimelapseWrapper {
private:
    string databaseName = "demomonitorTimelapseDb";
public:
    DatabaseTimelapseWrapper();
    std::vector<float> getValue(string key);
    void appendValue(string key);
    void deleteElement(string key);
    ~DatabaseTimelapseWrapper();
};


#endif /* DATABASE_TIMELAPSE_WRAPPER_H */

